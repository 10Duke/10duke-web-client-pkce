{
  "name": "@10duke/web-client-pkce",
  "version": "1.1.0",
  "description": "10Duke Identity and Entitlement client library for browser-based applications",
  "scripts": {
    "build": "tsc",
    "lint:fix": "eslint src/*.ts --fix",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "clear-eslint-cache": "rm -rf node_modules/.cache/eslint-loader"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:10Duke/10duke-web-client-pkce.git"
  },
  "files": [
    "lib/**/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "10Duke",
    "identity",
    "entitlement",
    "licensing",
    "client",
    "oidc",
    "OpenID Connect",
    "PKCE"
  ],
  "author": "10Duke",
  "license": "MIT",
  "dependencies": {
    "@types/debug": "^4.1.5",
    "debug": "^4.2.0",
    "jose-jwe-jws": "^0.2.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "typescript": "^3.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
